{
	"name": "@jhubbardsf/svelte-sortablejs",
	"version": "1.1.0",
	"author": "Josh Hubbard <josh@joshuahubbard.dev> (https://joshuahubbard.dev/)",
	"description": "A Svelte wrapper for the Sortable.js library",
	"repository": {
		"type": "git",
		"url": "git://github.com/jhubbardsf/svelte-sortablejs.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@bulatdashiev/svelte-slider": "^1.0.3",
		"@fortawesome/fontawesome-svg-core": "^1.2.26",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/sortablejs": "^1.10.7",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.4",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fontawesome-svelte": "^2.0.1",
		"postcss": "^8.4.12",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"sass": "^1.51.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.5",
		"svelte2tsx": "^0.5.9",
		"tailwindcss": "^3.0.23",
		"tslib": "^2.3.1",
		"typescript": "~4.9.3",
		"vite-plugin-sveld": "^1.0.3"
	},
	"type": "module",
	"dependencies": {
		"sortablejs": "^1.15.0"
	},
	"keywords": [
		"sortable",
		"reorder",
		"drag",
		"svelte-sortable",
		"sveltre",
		"sortablejs"
	],
	"license": "MIT"
}